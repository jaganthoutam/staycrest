    "seed:hotel-data": "node server/database/seeds/hotel_data_seed.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/staycrest.git"
  },
  "keywords": [
    "ai",
    "hotel",
    "loyalty",
    "voice",
    "agent"
  ],
  "author": "StayCrest Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/staycrest/issues"
  },
  "homepage": "https://github.com/yourusername/staycrest#readme",
